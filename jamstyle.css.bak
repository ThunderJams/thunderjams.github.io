body {
  background-color: lightblue;
  font-family: 'Trebuchet MS', sans-serif;
  text-align: center;
  
  background-image: url('/images/carpetBackground.png');
}

/* Header/Logo Title */
.header {
  padding: 60px;
  text-align: center;
  background: #AF38CD;
  color: black;
  font-size: 30px;
}

/* Page Content */
.content {
	padding:20px;
	color: white;
}

.button {
  border: none;
  color: black;
  color: black;
  padding: 16px 34px;
  text-align: center;
  text-decoration: none;
  display: inline-block;
  font-size: 16px;
  margin: 4px 4px;
  transition-duration: 0.4s;
  cursor: pointer;
  border-radius: 12px;
}

.button:hover{
 color: white;
}


