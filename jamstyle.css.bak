body {
  background-color: gray;
  font-family: 'Trebuchet MS', sans-serif;
  text-align: center;
  
  background-image: url('/images/carpetBackgroundBlur.png');
}

ul {
    text-align: center;
    list-style-position: inside;
}

/* Header/Logo Title */
#grad1 {
  background-color: #FBD98B; /* For browsers that do not support gradients */
  background-image: linear-gradient(to bottom right, #D9FDE3,#FBD98B, #F99B9C);
}

.header {
  overflow: hidden;
  
  padding: 20px 10px;
  background-color: black;
}

.header a {
  float: left;
  color: black;
  text-align: center;
  /*padding: 12px;*/
  text-decoration: none;
  font-size: 18px; 
  line-height: 25px;
  border-radius: 4px;
}

.header a.logo {
  font-size: 25px;
  font-weight: bold;
}

.header a {
    float: none;

  }
  
  .header-right {
    float: none;
  }

.header a.active {
  background-color: dodgerblue;
  color: white;
}

.header-right {
  float: right;
}

.banner-text {
	padding:20px;
	text-align: left;
	background: rgba(0,0,0,0.5);
}

.bodymain {
	padding:20px;
	background: rgba(0,0,0,0.5);
	float: left;
	margin-right:200px;
	margin-left:200px;
}

.bodytext {
	
	

}

.mobile {
	visibility:hidden;
	

}

.bannerFH {
  overflow: hidden;
  background-color: purple;
  padding: 20px 10px;
  background-image: url('/images/banners/FH.png');
  border: 4px solid black
}

.bannerHP {
  overflow: hidden;
  background-color: slategray;
  padding: 20px 10px;
  background-image: url('/images/banners/HP.PNG');
 border: 4px solid black
}

.bannerFW {
  overflow: hidden;
  background-color: MediumSeaGreen;
  padding: 20px 10px;
  background-image: url('/images/banners/FW.PNG');
border: 4px solid black
}

.bannerUW {
  overflow: hidden;
  background-color: MediumSlateBlue;
  padding: 20px 10px;
  background-image: url('/images/banners/UW.png');
border: 4px solid black
  
}

.responsive {
  width: 100%;
  max-width: 400px;
  height: auto;
  float: right;
}

a {
    
}



@media screen and (max-width: 500px) {
 
	
	

  
  
  .bannerFH {
    float: none;
    display: block;
    text-align: left;
  }
  
  .bannerHP {
    float: none;
    display: block;
    text-align: left;
  }
  
  .bannerFW {
    float: none;
    display: block;
    text-align: left;
  }
  
  .bannerUW {
    float: none;
    display: block;
    text-align: left;
  }
  
  .bodymain {
	padding:20px;
	background: rgba(0,0,0,0.5);
	float: left;
	margin-right:0px;
	margin-left:0px;
}


}

@media screen and (max-width: 1200px) {
.header a {
    float: none;

  }
  
  .header-right {
    float: none;
  }
  
  .bodymain {
	padding:20px;
	background: rgba(0,0,0,0.5);
	float: left;
	margin-right:0px;
	margin-left:0px;
}

.responsive {
  visibility:hidden;
  float: none;
  width: 0%;
}

.mobile {
	visibility:visible;
  width: 100%;
  max-width: 400px;
  height: auto;
}
}

.perma-img {
	visibility:visible;
  max-width: 100%;
  height: auto;
}

iframe {
	width: 420px;
  max-width: 100%;
  height: auto;
}

