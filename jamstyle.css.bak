body {
  background-color: gray;
  font-family: 'Overlock', 'Montserrat', 'Andal√© Mono', 'Trebuchet MS', sans-serif;
  text-align: center;
  
   background-image: url('/images/carpetBackgroundBlur.png'); 
   animation: pan 500s infinite;
   -webkit-animation: pan 500s infinite;
  -moz-animation: pan 500s infinite;
  -o-animation: pan 500s infinite;
  -ms-animation: pan 500s infinite;
  /*background-image: url('/images/bowling2.jpg');*/
}

@font-face {
  font-family: headerFont;
  src: url('/font/SnesItalic-1G9Be.tff');
}

h1{
	font-family: headerFont, 'Lulo Clean', 'Futura', 'Trebuchet MS', sans-serif;
}




@keyframes pan{
	0% {
    background-position: 0% 0%;
  }
  100% {
    background-position: 100% 100%;
  }
  
}

@-ms-keyframes pan{
		0% {
    background-position: 0% 0%;
  }
  100% {
    background-position: 100% 100%;
  }
  
}

@-o-keyframes pan{
	0% {
    background-position: 0% 0%;
  }

  100% {
    background-position: 100% 100%;
  }
}

@-webkit-keyframes pan{

 	0% {
    background-position: 0% 0%;
  }
  100% {
    background-position: 100% 100%;
  }
}

@-moz-keyframes pan{
	0% {
    background-position: 0% 0%;
  }
  100% {
    background-position: 100% 100%;
  }
}

